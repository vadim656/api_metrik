{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Категории",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "categories"
    },
    "Img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "parent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "id_parser": {
      "type": "string"
    },
    "minprice": {
      "type": "integer"
    },
    "maxprice": {
      "type": "integer"
    },
    "child": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "top_category"
    },
    "top_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "child"
    },
    "filter": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::filter.filter",
      "mappedBy": "kategorii"
    },
    "filter_cat": {
      "type": "json"
    },
    "tovaries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "parent_category"
    }
  }
}
